<Page x:Class="JasonMoney.UiApp.AccountsSummaryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:JasonMoney.UiApp"
      mc:Ignorable="d" 
      d:DesignHeight="450"
      d:DesignWidth="800"
      WindowTitle="Accounts"
      d:DataContext="{d:DesignInstance Type=local:AccountsSummaryViewModel}">
    <DockPanel>

        <StackPanel DockPanel.Dock="Left">
            <Button>New account</Button>
            <Separator />
            <Button>Pay bills</Button>
            <Separator />
            <Button>Analyze the reports</Button>
        </StackPanel>

        <Grid>
            
        </Grid>
        
    </DockPanel>
</Page>
